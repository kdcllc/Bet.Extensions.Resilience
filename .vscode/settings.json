{
    "files.trimTrailingWhitespace": true,
    "files.associations": {
        "*.*proj": "xml",
        "*.props": "xml",
        "*.targets": "xml",
        "*.tasks": "xml"
    },

    "dotnet-test-explorer.enableTelemetry": false,
    "dotnet-test-explorer.testProjectPath": "/test/**/*",
    "markdownlint.config": {
        "MD028": false,
        "MD025": {
            "front_matter_title": ""
        }
    }
}
